@model ToyForSI.Models.DevModel

@{
    ViewData["Title"] = "删除型号";
}

<h2>删除</h2>

<h3>确定删除此型号?</h3>
<div>
    <h4>型号</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayNameFor(model => model.devModelName)
        </dt>
        <dd>
            @Html.DisplayFor(model => model.devModelName)
        </dd>
        <dt>
            @Html.DisplayNameFor(model => model.brand)
        </dt>
        <dd>
            @Html.DisplayFor(model => model.brand.brandName)
        </dd>
        <dt>
            @Html.DisplayNameFor(model => model.equipmentType)
        </dt>
        <dd>
            @Html.DisplayFor(model => model.equipmentType.equipmentTypeName)
        </dd>
    </dl>
    
    <form asp-action="Delete">
        <input type="hidden" asp-for="devModelId" />
        <input type="submit" value="删除" class="btn btn-default" /> |
        <a asp-action="Index">返回列表</a>
    </form>
</div>
