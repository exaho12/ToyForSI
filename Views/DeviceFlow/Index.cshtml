@model ToyForSI.ViewModels.SiSN
@{
    ViewData["Title"] = "输入设备SN";
}
<h2>设备移交</h2>
<h4>输入设备SN</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="GetDevice">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="siSN" class="control-label"></label>
                <input asp-for="siSN" class="form-control" />
                <span asp-validation-for="siSN" class="text-danger"></span>
               </div>
            <div class="form-group">
                <input type="submit" value="下一步" class="btn btn-default" />
            </div>
        </form>
    </div>
</div>
